{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {"jsx": true}
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": "webpack.config.js",
    "rules": {
        "no-unused-vars":"error",
        "no-console":"error",
        "quotes": ["error", "single"],
        "jsx-quotes": ["error", "prefer-double"],
        "prefer-const": "error",
        "indent": ["error", 2],
        "comma-dangle": ["warn", "always-multiline"],
        "semi":["error", "always"]
}
}
